<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.myapplication.PlanDetailFragment"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <TextView
        android:id="@+id/plan_detail_yourrisk"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="@string/plan_detail_yourrisk"
        android:layout_marginStart="26dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/plan_detail_levelrisk"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/plan_detail_levelrisk"
        android:layout_width="125dp"
        android:layout_height="20dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="46dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/plan_detail_yourrisk" />

    <TextView
        android:id="@+id/plan_detail_yourex"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text="@string/plan_detail_yourex"
        android:layout_marginStart="26dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="21dp"
        app:layout_constraintRight_toRightOf="@+id/plan_detail_yourrisk"
        app:layout_constraintBaseline_toBaselineOf="@+id/plan_detail_levelex"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/plan_detail_levelex"
        android:layout_width="154dp"
        android:layout_height="24dp"
        android:text="text"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="11dp"
        android:layout_marginTop="38dp"
        app:layout_constraintTop_toBottomOf="@+id/plan_detail_levelrisk"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/plan_detail_yourex" />

</android.support.constraint.ConstraintLayout>
