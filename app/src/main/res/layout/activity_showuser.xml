<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.myapplication.ShowUserActivity">

    <TextView
        android:id="@+id/showuser_fullname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_fullname"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="58dp"
        android:layout_marginEnd="58dp"
        app:layout_constraintRight_toRightOf="@+id/showuser_height_value"
        android:layout_marginTop="94dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="58dp"
        android:layout_marginRight="58dp" />

    <TextView
        android:id="@+id/showuser_gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_gender"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/showuser_fullname"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_fullname" />

    <TextView
        android:id="@+id/showuser_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_age"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="10dp"
        app:layout_constraintRight_toRightOf="@+id/showuser_fullname_value"
        app:layout_constraintBaseline_toBaselineOf="@+id/showuser_gender_value"
        android:layout_marginRight="10dp" />

    <TextView
        android:id="@+id/showuser_height"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_height"
        android:layout_marginStart="57dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="57dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/showuser_bloodsugar"
        android:layout_marginTop="109dp"
        app:layout_constraintTop_toBottomOf="@+id/showuser_fullname_value"
        android:layout_marginBottom="109dp" />

    <TextView
        android:id="@+id/showuser_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_weight"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="22dp"
        app:layout_constraintRight_toLeftOf="@+id/showuser_weight_value"
        app:layout_constraintTop_toTopOf="@+id/showuser_height_value"
        android:layout_marginRight="22dp" />

    <TextView
        android:id="@+id/showuser_waistline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_waistline"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_height"
        android:layout_marginLeft="1dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="1dp"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/showuser_height" />

    <TextView
        android:id="@+id/showuser_bloodsugar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_bloodsugar"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_pressure"
        android:layout_marginLeft="1dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_cholesterol"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="21dp" />

    <TextView
        android:id="@+id/showuser_cholesterol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_cholesterol"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_bloodsugar"
        android:layout_marginLeft="1dp"
        android:layout_marginStart="1dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/showuser_cholesterol_value" />

    <TextView
        android:id="@+id/showuser_pressure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_pressure"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_bloodsugar"
        android:layout_marginStart="57dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="23dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="57dp" />

    <Button
        android:id="@+id/showuser_submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_submit"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="39dp"
        android:layout_marginLeft="113dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.622"
        android:layout_marginStart="113dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:id="@+id/showuser_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_edit"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/showuser_submit"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_cholesterol"
        app:layout_constraintTop_toTopOf="@+id/showuser_submit"
        android:layout_marginLeft="15dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/showuser_fullname_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_age"
        android:layout_marginStart="48dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="26dp"
        app:layout_constraintLeft_toRightOf="@+id/showuser_fullname"
        android:layout_marginLeft="48dp" />

    <TextView
        android:id="@+id/showuser_gender_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintRight_toLeftOf="@+id/showuser_fullname_value"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="4dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/showuser_gender"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="4dp" />

    <TextView
        android:id="@+id/showuser_age_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="28dp"
        android:layout_marginTop="1dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/showuser_age"
        app:layout_constraintLeft_toRightOf="@+id/showuser_fullname_value"
        android:layout_marginLeft="28dp" />

    <TextView
        android:id="@+id/showuser_height_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_waistline"
        android:layout_marginStart="27dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="13dp"
        app:layout_constraintLeft_toRightOf="@+id/showuser_waistline"
        android:layout_marginLeft="27dp" />

    <TextView
        android:id="@+id/showuser_weight_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="40dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/showuser_weight"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="40dp" />

    <TextView
        android:id="@+id/showuser_waistline_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/showuser_height_value"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/showuser_height_value"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_height_value" />

    <TextView
        android:id="@+id/showuser_pressure_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_bloodsugar_value"
        android:layout_marginStart="26dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="21dp"
        app:layout_constraintLeft_toRightOf="@+id/showuser_waistline_value"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/showuser_bloodsugar_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_cholesterol"
        app:layout_constraintRight_toRightOf="@+id/showuser_pressure_value"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="21dp"
        app:layout_constraintLeft_toLeftOf="@+id/showuser_pressure_value" />

    <TextView
        android:id="@+id/showuser_cholesterol_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/showuser_submit"
        android:layout_marginStart="26dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="42dp"
        app:layout_constraintLeft_toRightOf="@+id/showuser_cholesterol"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/showuser_text_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/showuser_text_head"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="31dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/showuser_fullname"
        android:layout_marginLeft="-21dp" />

</android.support.constraint.ConstraintLayout>
