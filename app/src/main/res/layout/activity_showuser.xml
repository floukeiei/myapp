<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_brown9"
    tools:context="com.example.user.myapplication.ShowUserActivity">

    <TextView
        android:id="@+id/showuser_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="124dp"
        android:text="@string/showuser_age"
        app:layout_constraintBaseline_toBaselineOf="@+id/showuser_gender_value"
        app:layout_constraintRight_toRightOf="@+id/showuser_fullname_value"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/showuser_submit"
        android:layout_width="75dp"
        android:layout_height="60dp"
        android:layout_marginBottom="36dp"
        android:layout_marginLeft="113dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/ic_check"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.847"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/showuser_edit"
        android:layout_width="77dp"
        android:layout_height="68dp"
        android:background="@drawable/ic_edit"
        app:layout_constraintBottom_toBottomOf="@+id/showuser_submit"
        app:layout_constraintTop_toTopOf="@+id/showuser_submit"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/showuser_fullname_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:text="TextView"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/showuser_age"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_editor_absoluteX="121dp" />

    <TextView
        android:id="@+id/showuser_gender_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="40dp"
        android:text="TextView"
        app:layout_constraintRight_toLeftOf="@+id/showuser_fullname_value"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_editor_absoluteY="109dp" />

    <TextView
        android:id="@+id/showuser_age_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/showuser_fullname_value"
        app:layout_constraintTop_toTopOf="@+id/showuser_age"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="187dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="250dp"
        tools:layout_editor_absoluteY="187dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="48dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/ic_signal_bars"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="163dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="55dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/ic_flow_chart"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="170dp" />

</android.support.constraint.ConstraintLayout>
