<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.myapplication.RiskActivity">

    <TextView
        android:id="@+id/planex_gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_gender"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="71dp"
        app:layout_constraintBottom_toBottomOf="@+id/planex_gender_value"
        android:layout_marginEnd="71dp"
        app:layout_constraintRight_toRightOf="@+id/planex_heartrateav"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/planex_gender_value"
        android:layout_marginLeft="71dp"
        android:layout_marginRight="71dp" />

    <TextView
        android:id="@+id/planex_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_age"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="71dp"
        app:layout_constraintBottom_toBottomOf="@+id/planex_age_value"
        android:layout_marginEnd="70dp"
        app:layout_constraintRight_toRightOf="@+id/planex_heartrateav"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/planex_age_value"
        android:layout_marginLeft="71dp"
        android:layout_marginRight="70dp"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/planex_timeused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_timeused"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/planex_distanceused"
        app:layout_constraintBaseline_toBaselineOf="@+id/planex_timeused_value"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/planex_age" />

    <TextView
        android:id="@+id/planex_heartrateav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_heartrateav"
        android:layout_marginStart="34dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/planex_heartrateav_value"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="34dp" />

    <TextView
        android:id="@+id/planex_distanceused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_distanceused"
        android:layout_marginStart="70dp"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="49dp"
        app:layout_constraintTop_toBottomOf="@+id/planex_heartrateav" />

    <TextView
        android:id="@+id/planex_gender_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/planex_timeused_value"
        android:layout_marginTop="57dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/planex_timeused_value"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/planex_age_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="52dp"
        app:layout_constraintTop_toBottomOf="@+id/planex_gender_value"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/planex_gender_value" />

    <TextView
        android:id="@+id/planex_timeused_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@+id/planex_age_value"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/planex_heartrateav_value" />

    <TextView
        android:id="@+id/planex_heartrateav_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/planex_distanceused_value"
        android:layout_marginTop="68dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="67dp"
        app:layout_constraintTop_toTopOf="@+id/planex_timeused_value"
        app:layout_constraintLeft_toRightOf="@+id/planex_heartrateav"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/planex_distanceused_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/planex_vo2max_value"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="40dp"
        app:layout_constraintLeft_toLeftOf="@+id/planex_vo2max_value" />

    <TextView
        android:id="@+id/planex_vo2max"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_vo2max"
        android:layout_marginStart="1dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/planex_vo2max_value"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/planex_distanceused"
        android:layout_marginLeft="1dp" />

    <TextView
        android:id="@+id/planex_vo2max_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/planex_heartrateav_value"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="103dp"
        app:layout_constraintLeft_toLeftOf="@+id/planex_heartrateav_value" />

    <Button
        android:id="@+id/planex_previous"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_previous"
        android:layout_marginLeft="65dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="29dp" />

    <Button
        android:id="@+id/planex_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/planex_next"
        android:layout_marginRight="81dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="29dp" />
</android.support.constraint.ConstraintLayout>
